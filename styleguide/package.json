{
  "name": "@lojaintegrada/admin-components",
  "version": "0.8.1",
  "author": "Loja Integrada Front-End Team",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "TAILWIND_MODE=build NODE_ENV=production tsdx build",
    "build:complete": "node scripts/extract-selectors.js",
    "test": "yarn test:jest && yarn test:cypress:ci",
    "test:jest": "tsdx test --passWithNoTests",
    "test:cypress": "cypress open-ct",
    "test:cypress:ci": "cypress run-ct",
    "lint": "tsdx lint",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "es5"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "module": "dist/admin-components.esm.js",
  "size-limit": [
    {
      "path": "dist/admin-components.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/admin-components.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@cypress/react": "^5.9.0",
    "@cypress/webpack-dev-server": "^1.3.1",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@size-limit/preset-small-lib": "^4.10.1",
    "@storybook/addon-a11y": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@storybook/storybook-deployer": "^2.8.10",
    "@storybook/testing-react": "^0.0.21",
    "@testing-library/react": "^11.2.7",
    "@types/node": "^15.12.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-modal": "^3.12.1",
    "@types/react-table": "^7.7.1",
    "@types/react-text-mask": "^5.4.8",
    "@types/styled-components": "^5.1.14",
    "autoprefixer": "^9",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-name-mapper": "^1.2.0",
    "chromatic": "^5.9.2",
    "cypress": "^7.4.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^5.1.3",
    "identity-obj-proxy": "^3.0.0",
    "list-selectors": "^2.0.0",
    "postcss": "^8",
    "postcss-loader": "^4.2.0",
    "react-is": "^17.0.1",
    "rollup-plugin-postcss": "^4.0.0",
    "size-limit": "^4.11.0",
    "storybook-addon-designs": "^6.0.1",
    "tailwindcss": "^2.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3",
    "webpack-dev-server": "^3.11.2"
  },
  "peerDependency": {
    "react": ">=17.0.1",
    "react-dom": ">=17.0.1"
  },
  "dependencies": {
    "@lojaintegrada/tailwindcss-config": "^1.3.0",
    "@tippyjs/react": "^4.2.5",
    "@types/react-select": "^4.0.17",
    "react-modal": "^3.14.3",
    "react-select": "^4.3.1",
    "react-table": "^7.7.0",
    "react-text-mask": "^5.4.3",
    "react-toastify": "^7.0.4",
    "styled-components": "^5.3.1"
  }
}
