{
  "name": "@loja-integrada/admin-components",
  "version": "0.18.30",
  "author": "Loja Integrada Front-End Team",
  "license": "MIT",
  "repository": {
    "url": "lojaintegrada/admin-components",
    "directory": "styleguide"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "TAILWIND_MODE=build NODE_ENV=production tsdx build",
    "build:complete": "node scripts/extract-selectors.js",
    "test": "yarn test:jest && yarn test:cypress:ci",
    "test:sonar": "yarn test:cypress:ci && node sonar-config.js",
    "test:jest": "tsdx test --passWithNoTests",
    "test:cypress": "cypress open-ct",
    "test:cypress:ci": "cypress run-ct",
    "lint": "tsdx lint",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "es5"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "module": "dist/admin-components.esm.js",
  "size-limit": [
    {
      "path": "dist/admin-components.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/admin-components.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.13.0",
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/react": "^5.12.4",
    "@cypress/webpack-dev-server": "^1.8.4",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/client-api": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/preview-web": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/storybook-deployer": "^2.8.11",
    "@storybook/testing-react": "^1.3.0",
    "@testing-library/react": "^11.2.7",
    "@types/node": "^15.12.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-modal": "^3.12.1",
    "@types/react-table": "^7.7.1",
    "@types/react-text-mask": "^5.4.8",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.2",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-module-name-mapper": "^1.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "chromatic": "^6.5.4",
    "css-loader": "5.2.6",
    "cypress": "^9.6.0",
    "cypress-sonarqube-reporter": "^1.11.0",
    "eslint-plugin-prettier": "^3.4.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^5.1.3",
    "identity-obj-proxy": "^3.0.0",
    "postcss": "^8.4.12",
    "postcss-loader": "^4.2.0",
    "postcss-nesting": "^11.2.1",
    "prettier": "^2.6.2",
    "react-is": "^18.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.59.3",
    "sass-loader": "10.1.1",
    "size-limit": "^7.0.8",
    "storybook-addon-designs": "^6.3.1",
    "storybook-dark-mode": "^2.1.1",
    "style-loader": "2.0.0",
    "tailwindcss": "^3.1.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "webpack": "^5.76.0",
    "webpack-dev-server": "^4.9.0"
  },
  "peerDependency": {
    "@loja-integrada/tailwindcss-config": ">=1.5.0",
    "react": ">=17.0.1",
    "react-dom": ">=17.0.1",
    "tailwindcss": ">=3.1.0"
  },
  "dependencies": {
    "@loja-integrada/tailwindcss-config": "^1.7.1",
    "@tippyjs/react": "^4.2.5",
    "@types/react-select": "^4.0.17",
    "babel-jest": "^29.2.2",
    "child-process": "^1.0.2",
    "dotenv": "^16.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-modal": "^3.14.3",
    "react-select": "^4.3.1",
    "react-table": "^7.7.0",
    "react-text-mask": "^5.4.3",
    "react-toastify": "^7.0.4",
    "sonarqube-scanner": "^2.8.2"
  }
}
